	desc = "A program install disk. This one is CamView, the latest version which allows modular updates for Cameras, allowing for custom Camera Network allowances!"
	icon = 'icons/obj/stock_parts.dmi'
	icon_state = "datadisk_seccam"
	spawn_files = list(/datum/file/program/security)

/obj/item/weapon/disk/file/card
	name = "Outpost 13 Access Modifier"
	desc = "A program install disk. This one has the program for the NanoTrasen Outpost ID Access Software. Apparently it's a known bug that some unused accesses are retained on here, but it's a nonconcern due to the redundency in airlock programming."
	icon = 'icons/obj/stock_parts.dmi'
	icon_state = "datadisk2"
	spawn_files = list(/datum/file/program/card_comp)

/obj/item/weapon/disk/file/robot
