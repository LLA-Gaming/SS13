/obj/item/weapon/disk/file/aifixer
	name = "HI Sys-Recovery"
	desc = "A program install disk. Ever since NT's AI secrets got leaked, they've been renamed Human Intelligneces (HI), but are still erronously named AIs by some. Still, this program allows for better fixing of those HI and improves it by allowing for law review and wiping of the HI while it's loaded in memory."
	icon = 'icons/obj/stock_parts.dmi'
	icon_state = "datadisk_arcade"
	spawn_files = list(/datum/file/program/aifixer)

/obj/item/weapon/disk/file/comm
	name = "COMMaster V3"
	desc = "This program allows for contacting Central Command, vital for evacuation of an outpost or space station. This is assuming you have the standard Subspace Reciever needed, however, along with access to the Command Center itself to even login."
	icon = 'icons/obj/stock_parts.dmi'
	icon_state = "datadisk_comm"
	spawn_files = list(/datum/file/program/communications)

/obj/item/weapon/disk/file/cameras
